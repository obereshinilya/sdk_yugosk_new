{
    "name": "unicodeveloper/laravel-sessiontimeout",
    "description": "A middleware to deal with your sessiontimeout problems in Laravel 5",
    "keywords": [
        "laravel",
        "laravel 5",
        "laravel-sessiontimeout",
        "session",
        "timeout"
    ],
    "homepage": "https://github.com/unicodeveloper/laravel-sessiontimeout",
    "license": "MIT",
    "authors": [
        {
            "name": "Prosper Otemuyiwa",
            "email": "prosperotemuyiwa@gmail.com",
            "homepage": "https://twitter.com/unicodeveloper",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Unicodeveloper\\Session\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unicodeveloper\\Session\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
